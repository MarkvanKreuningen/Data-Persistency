<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class table="ov_chipkaart_product">
        <id name="kaartNumber" type="int" access="field">
            <column name="kaart_nummer" />
            <generator class="assigned" />
        </id>

        <property name="productNumber" type="int" access="field">
            <column name="product_nummer" />
        </property>

        <property name="status" type="java.lang.String" access="field">
            <column name="status" />
        </property>

        <property name="woonplaats" type="java.lang.String" access="field">
            <column name="WOONPLAATS" />
        </property>

        <property name="reizigerId" type="int" access="field">
            <column name="REIZIGER_ID" />
        </property>

    </class>
</hibernate-mapping>
